FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
	
	front
    {
        type            empty;
    }
	back
    {
        type            empty;
    }
	
    upper_wall
    {
        type            zeroGradient;
    }
	
    lower_wall
    {
        type            zeroGradient;
    }
	cylinder
    {
        type            zeroGradient;
    }

    internal_1.1
    {
        type            cyclic;
        mappedPatch     internal_2.1;
    }
    internal_2.1
    {
        type            cyclic;
        mappedPatch     internal_1.1;
    }
    internal_2.2
    {
        type            cyclic;
        mappedPatch     internal_3.1;
    }
    internal_3.1
    {
        type            cyclic;
        mappedPatch     internal_2.2;
    }
}


